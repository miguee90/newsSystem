<header>
	<ng-include src="'views/includes/menuNoClima.ejs'"></ng-include>
</header>
<main>
	<div class="row" style="margin:0px;">
		<div class="col-sm-12 col-xs-12 col-lg-12 notaP" ng-click="anoticia(portada[0].nota.alias,portada[0].nota.idNotas)" ng-style="backImage">
			<div class="smoke">
				<h1 class="col-xs-12 col-sm-12 col-lg-4 col-lg-offset-7">NEGOCIOS</h1>
				<a href="https://www.facebook.com/GlobalMediaSLP/?fref=ts">
					<img class=" col-lg-offset-10" alt="logo facebook" src="images/common/facebook-white.png">
				</a>
				<a href="https://twitter.com/globalmediamx">
					<img class="" alt="logo twitter" src="images/common/twitter-white.png">
				</a>
				<a href="https://www.youtube.com/channel/UCqF55I3fwX8fs0AEgaIvNQQ">
					<img class="" alt="logo youtube" src="images/common/youtube-white.png">
				</a>
				<div style="" class="col-xs-12 col-sm-12 col-lg-12 encabezado">
					<p class="ti">{{portada[0].nota.titulo}}</p>
					<p class="col-lg-12 re">{{portada[0].nota.resumen}}</p>
				</div>
			</div>
		</div>
		<div ng-click="goto()" class="col-lg-1 boton-baja  hidden-sm hidden-xs">
			<img class="img-responsive" alt="baja" src="images/common/chevron-down.png">
		</div>
	</div>
	<div class="row" style="margin:0px; padding:0px;">
		<div class="col-lg-10 col-lg-offset-1">
			<div class="row" id="notas">
				<div class="col-lg-6">
					<div class="row">
						<a ui-sref="noticiaId({alias: portada[1].nota.alias, id:portada[1].nota.idNotas})">
							<div  class="bode-inf" id="segunda" class="col-lg-12">
								<img class="img-responsive" alt="segunda" ng-src="images/multimedia/{{portada[1].imagen.imgSeccionF}}">
								<p>{{portada[1].nota.fecha_inicio | date:'yyyy-MM-dd HH:mm'}}</p>
								<h3>{{portada[1].nota.titulo}}</h3>
								<p>{{portada[1].nota.resumen}}</p>
							</div>
						</a>
					</div>
					<div class="row bode-inf">
						<a ui-sref="noticiaId({alias: portada[2].nota.alias, id:portada[2].nota.idNotas})" >
							<div id="tercera" class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
								<h3>{{portada[2].nota.titulo}}</h3>
								<p>{{portada[2].nota.resumen}}</p>
							</div>
							<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
								<img style="margin-top:30px; height:80px;" class="img-responsive" alt="tercera nota local" ng-src="images/multimedia/{{portada[2].imagen.imagenPortada}}">
							</div>
						</a>
					</div>
					<div class="row bode-inf">
						<a ui-sref="noticiaId({alias: portada[3].nota.alias,id:portada[3].nota.idNotas})">
							<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
								<img style="margin-top:30px; height:80px;" class="img-responsive" alt="tercera nota local" ng-src="images/multimedia/{{portada[3].imagen.imagenPortada}}">
							</div>
							<div id="cuarta" class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
								<h3>{{portada[3].nota.titulo}}</h3>
								<p>{{portada[3].nota.resumen}}</p>
							</div>
						</a>
					</div>
					<div class="row bode-inf">
						<a ui-sref="noticiaId({alias: portada[4].nota.alias,id:portada[4].nota.idNotas})" >
							<div id="tercera" class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
								<h3>{{portada[4].nota.titulo}}</h3>
								<p>{{portada[4].nota.resumen}}</p>
							</div>
							<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
								<img style="margin-top:30px; height:80px;" class="img-responsive" alt="tercera nota local" ng-src="images/multimedia/{{portada[4].imagen.imagenPortada}}">
							</div>
						</a>
					</div>
					<div class="row bode-inf">
						<a ui-sref="noticiaId({alias: portada[5].nota.alias, id:portada[5].nota.idNotas})">
							<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
								<img style="margin-top:30px; height:80px;" class="img-responsive" alt="tercera nota local" ng-src="images/multimedia/{{portada[5].imagen.imagenPortada}}">
							</div>
							<div id="cuarta" class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
								<h3>{{portada[5].nota.titulo}}</h3>
								<p>{{portada[5].nota.resumen}}</p>
							</div>
						</a>
					</div>

					<!--NOTICIAS EXTRAS-->
					<div class="row bode-inf">
						<a ng-repeat="nota in notasMas" ui-sref="noticiaId({alias: nota.alias, id:nota.idNotas})">
							<div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
								<img style="margin-top:30px; height:80px;" class="img-responsive" alt="tercera nota local" ng-src="images/multimedia/{{nota.imagenes.imagenPortada}}">
							</div>
							<div id="cuarta" class="col-lg-8 col-md-8 col-sm-8 col-xs-8">
								<h3>{{nota.titulo}}</h3>
								<p>{{nota.resumen}}</p>
							</div>
						</a>
					</div>
					<div class="row">
						<a href="" ng-click="cargaMas()">
							<div class="col-lg-6 col-lg-offset-6 cargar-mas">
								<p style="margin-top:15px; text-align:center;">CARGAR M√ÅS CONTENIDO</p>
							</div>
						</a>
					</div>
				</div>
				<!--SECCION DERECHA-->
				<div class="col-lg-6 no-marpad">
					<div class="row no-marpad">
						<div class="col-lg-8">
							<div class="row no-marpad">
								<div style="margin-top:70px;" class="col-lg-12 cargar-mas">
									<p class="tendencia">TENDENCIA EN NEGOCIOS</p>
								</div>
							</div>
							<div class="row">
								<a ui-sref="noticiaId({alias:top.nota.alias, id:top.nota.idNotas})" ng-repeat="top in topNews">
									<div id="tops" class="col-lg-12 bode-inf">
										<img class="img-responsive" alt="top news" ng-src="images/multimedia/{{top.imagen.imgSeccionF}}">
										<p>{{top.nota.fecha_inicio | date:'yyyy-MM-dd HH:mm'}}</p>
										<h4>{{top.nota.titulo}}</h4>
										<p>{{top.nota.resumen}}</p>
									</div>
								</a>
							</div>
							<div class="row">
								<a ui-sref="Trafico">
									<div class="col-lg-12" style="margin-top:30px;">
										<img class="img-responsive" alt="trafico san luis" src="images/common/trafico.jpg">
									</div>
								</a>
							</div>
						</div>
						<div class="col-lg-4" style="margin-top:70px;">
							<div class="col-lg-12 banner-derecho" >
								<img alt="internacional" class="full-img hidden-xs" ng-src="{{slidesD[0].path.substr(6)}}">
							</div>
							<div class="col-lg-12 banner-derecho" style="margin-top:20px;">
								<img alt="internacional" class="full-img hidden-xs" ng-src="{{slidesD[2].path.substr(6)}}">
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</main>
<footer>
	<ng-include src="'views/includes/footer.ejs'"></ng-include>
</footer>